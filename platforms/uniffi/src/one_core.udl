namespace one_core {
  OneCore initialize_core(string data_dir_path);
};

[Error]
enum DataLayerError {
  "GeneralRuntimeError", "AlreadyExists", "IncorrectParameters", "RecordNotFound", "RecordNotUpdated", "Other"
};

interface OneCore {
  [Throws=DataLayerError]
  string create_org();
  Version version();
};

dictionary Version {
    string target;
    string build_time;
    string branch;
    string tag;
    string commit;
    string rust_version;
    string pipeline_id;
};
